# {{ ansible_managed }}

Host mac
    User aetf
    HostName aetf-arch-mac.zt.unlimited-code.works
    Port 22
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host xps
    User aetf
    HostName aetf-arch-xps.zt.unlimited-code.works
    Port 59901
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host mcsh
    User root
    HostName mcsh.unlimited-code.works
    Port 8020
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host github
    User git
    HostName github.com
    ForwardX11 no
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host archvps
    User aetf
    HostName archvps.hosts.unlimited-code.works
    Port 59901
    ForwardX11 no
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host conflux
    User peifeng
    HostName conflux-login1.arc-ts.umich.edu
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    LocalForward 9999 localhost:8888
    LocalForward 9013 localhost:9013
    LocalForward 9014 localhost:9014
    LocalForward 9015 localhost:9015
    LocalForward 9016 localhost:9016
    LocalForward 9017 localhost:9017
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host login.engin.umich.edu
    User peifeng
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host symbiotic
    User peifeng
    HostName symbiotic.eecs.umich.edu
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host abacus
    User peifeng
    HostName abacus.eecs.umich.edu
    Port 39901
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    LocalForward 8080 localhost:8080
    LocalForward 8000 localhost:8000
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

# for docker connection, disable control path
Host docker-cloudlab
    HostName nfs.dyninfer.gaia-pg0.clemson.cloudlab.us
Match final host *.cloudlab.us originalhost docker-cloudlab
    ControlMaster no
    ControlPath none
    ControlPersist no
Host clockwork-builder
    User user
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    CheckHostIP no
    HostName localhost
    Port 2222

Host clockwork-builder-proxy
    HostName clgpu022.clemson.cloudlab.us

Host clockwork-builder-remote
    User user
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    CheckHostIP no
    # eith port or proxy command, depends on local or remote
    ProxyCommand ssh -W localhost:2223 clockwork-builder-proxy

Host clockwork-builder-remote-0
    User user
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    CheckHostIP no
    # eith port or proxy command, depends on local or remote
    ProxyCommand ssh -W localhost:2223 clockwork-builder-proxy
Host clockwork-builder-remote-1
    User user
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    CheckHostIP no
    # eith port or proxy command, depends on local or remote
    ProxyCommand ssh -W localhost:2224 clockwork-builder-proxy

Host clnode???
    HostName %h.clemson.cloudlab.us
Host clgpu???
    HostName %h.clemson.cloudlab.us
Host nfs
    HostName %h.dyninfer.gaia-pg0.clemson.cloudlab.us
Host node-?
    HostName %h.dyninfer.gaia-pg0.clemson.cloudlab.us
Match final host *.cloudlab.us
    User peifeng
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%C-%n
    ControlPersist yes

Host cc
    HostName 129.114.108.31
Match final host 129.114.108.*
    User cc
    UserKnownHostsFile /dev/null
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
